<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Fancytree -->
        <service
            id="cmf_tree_ui.view.fancy_tree"
            class="Symfony\Cmf\Bundle\TreeUiBundle\Tree\View\FancyTreeView"
            >
            <argument type="service" id="twig"/>
            <argument type="service" id="router"/>
            <tag name="cmf_tree_ui.view" alias="fancy_tree"/>
        </service>

        <!-- Dynatree -->
        <service
            id="cmf_tree_ui.view.dynatree"
            class="Symfony\Cmf\Bundle\TreeUiBundle\Tree\View\DynatreeView"
            >
            <argument type="service" id="twig"/>
            <argument type="service" id="router"/>
            <tag name="cmf_tree_ui.view" alias="dyna_tree"/>
        </service>

        <!-- ElFinder -->
        <service
            id="cmf_tree_ui.view.elfinder"
            class="Symfony\Cmf\Bundle\TreeUiBundle\Tree\View\ElFinderView"
            >
            <argument type="service" id="twig"/>
            <argument type="service" id="router"/>
            <tag name="cmf_tree_ui.view" alias="el_finder"/>
        </service>
    </services>
</container>
