<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service 
            id="cmf_tree_ui.metadata.factory"
            class="Metadata\MetadataFactory"
        >
            <argument 
                type="service" 
                id="cmf_tree_ui.metadata.annotations_driver"
            />
        </service>

        <service
            id="cmf_tree_ui.metadata.driver.annotation"
            class="Symfony\Cmf\Bundle\TreeUiBundle\Tree\Metadata\Driver\AnnotationDriver"
        />
    </services>
</container>


