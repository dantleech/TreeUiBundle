<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="cmf_tree_ui.tree_type_class">Symfony\Cmf\Bundle\TreeUiBundle\Form\Type\TreeUiType</parameter>
    </parameters>

    <services>
        <service id="cmf_tree_ui.tree_type_class" class="%cmf_tree_ui.tree_type_class%">
            <argument type="service" id="doctrine_phpcr"/>

            <tag name="form.type" alias="cmf_tree_ui_tree" />
        </service>
    </services>
</container>
